{
  "name": "create-uni",
  "type": "module",
  "version": "0.0.2-beta.3",
  "packageManager": "pnpm@8.6.0",
  "description": "An easy way to start a uni-app project",
  "author": "FliPPeDround <flippedround@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/FliPPeDround",
  "homepage": "https://github.com/FliPPeDround/create-uni#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FliPPeDround/create-uni.git"
  },
  "bugs": "https://github.com/FliPPeDround/create-uni/issues",
  "keywords": [
    "uni",
    "uni-app",
    "create-uni",
    "cli",
    "vue",
    "miniapp"
  ],
  "sideEffects": false,
  "bin": {
    "create-uni": "outfile.cjs"
  },
  "files": [
    "outfile.cjs",
    "template"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@antfu/ni": "^0.21.3",
    "@antfu/utils": "^0.7.4",
    "@types/ejs": "^3.1.2",
    "@types/node": "^18.16.16",
    "@types/prompts": "^2.4.4",
    "bumpp": "^9.1.0",
    "ejs": "^3.1.9",
    "esbuild": "^0.18.10",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "kolorist": "^1.8.0",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.31.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
